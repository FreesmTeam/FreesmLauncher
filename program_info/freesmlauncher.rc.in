#ifndef WIN32_LEAN_AND_MEAN
#define WIN32_LEAN_AND_MEAN
#endif
#include <windows.h>

IDI_ICON1               ICON    DISCARDABLE     "freesmlauncher.ico"
1 RT_MANIFEST "freesmlauncher.manifest"

VS_VERSION_INFO VERSIONINFO
FILEVERSION     @Launcher_VERSION_NAME4_COMMA_WINDOWS@
FILEOS          VOS_NT_WINDOWS32
FILETYPE        VFT_APP
BEGIN
        BLOCK "StringFileInfo"
        BEGIN
                BLOCK "000004b0"
                BEGIN
                        VALUE "CompanyName", "Freesm Launcher"
                        VALUE "FileDescription", "Freesm Launcher"
                        VALUE "FileVersion", "@Launcher_VERSION_NAME4_WINDOWS@"
                        VALUE "ProductName", "Freesm Launcher"
                        VALUE "ProductVersion", "@Launcher_VERSION_NAME4_WINDOWS@"
                END
        END
        BLOCK "VarFileInfo"
        BEGIN
                VALUE "Translation", 0x0000, 0x04b0 // Unicode
        END
END
